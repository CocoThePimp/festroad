<div class= "col">
  <div class= "row justify-content-between mr-1 ml-1">
    <h5><%= fest.title %><h5>
    <p>Du <%= fest.start_date.strftime("%d/%m/%Y") %> au <%= fest.end_date.strftime("%d/%m/%Y") %><p>
  </div>
    <% if fest.description != nil %>
    <p>Description: <%= fest.description %> </p>
    <% end %>
    <% if fest.website != nil %>
    <p>Site web: <%= link_to fest.website, fest.website %></p>
    <% end %>
</div>